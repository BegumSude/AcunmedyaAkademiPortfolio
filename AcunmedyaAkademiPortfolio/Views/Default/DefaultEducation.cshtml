@model List<AcunmedyaAkademiPortfolio.Models.TblEducation>

<h3 class="resume-title">Eğitimlerim</h3>
                    @foreach (var item in Model)
                    {
                        <div class="resume-item">
                            <h4>@item.Department</h4>
                            <h5>@item.StartYear - @item.EndYear</h5>
                            <p><em>@item.SchoolName</em></p>

                        </div>
                    }

              

